{
	"Name": "websysd",
	"Environment": {
		"WORKSPACE": "websysd"
	},
	"Tasks": [
		{
			"Name": "dp-filter-api",
			"Command": "sh start-go-service.sh",
			"Environment": {
				"PATH": "/bin:/usr/bin:/usr/local/bin"
			},
			"Service": true,
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		},
		{
			"Name": "dp-code-list-api",
			"Command": "sh start-go-service-no-makefile.sh",
			"Environment": {
				"PATH": "/bin:/usr/bin:/usr/local/bin"
			},
			"Service": true,
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		},
		{
			"Name": "dp-hierarchy-api",
			"Command": "sh start-go-service-no-makefile.sh",
			"Environment": {
				"PATH": "/bin:/usr/bin:/usr/local/bin"
			},
			"Service": true,
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		},
		{
			"Name": "dp-compose",
			"Command": "sh start-dp-compose.sh",
			"Environment": {
				"PATH": "/bin:/usr/bin:/usr/local/bin"
			},
			"Service": true,
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		},
		{
			"Name": "dp-import-api",
			"Command": "sh start-go-service.sh",
			"Environment": {
				"PATH": "/bin:/usr/bin:/usr/local/bin"
			},
			"Service": true,
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		},
		{
			"Name": "dp-recipe-api",
			"Command": "sh start-go-service.sh",
			"Environment": {
				"PATH": "/bin:/usr/bin:/usr/local/bin"
			},
			"Service": true,
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		}
	]
}
